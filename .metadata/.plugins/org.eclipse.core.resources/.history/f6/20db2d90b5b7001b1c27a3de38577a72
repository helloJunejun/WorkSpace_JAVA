package com.work.view;

public class GuGuDanTest {
	public static void main(String[] args) {
		System.out.println("\n1. for 구문을 활용한 구구단");
		
		int i;
		int j;
		for(i = 2; i <= 9; i++) {
			System.out.print(i + "단\t");
		}
		System.out.print("\n");
		
		for(j = 1; j <= 9; j++) {
			for(i = 2; i <= 9; i++) {
				
				System.out.print(i+ "*" + j + "=" + i * j+"\t");
			
			}		
			System.out.println();
	}
				
}
		
}
	
